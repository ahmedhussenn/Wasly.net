<!DOCTYPE html>
<html lang="en">


@{
    Layout = null;
}
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Project</title>
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/all.min.css">
    <link rel="stylesheet" href="css/Style.css">
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Wasly.net.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="Style.css">
    <style>

        :root {
            --dark-color: #EFECE7;
            --gary: #8C7040;
            --section: #EFECE7;
        }

        body {
            font-family: "Roboto",;
            background-color: #1B3252;
        }

        .main-btn {
            color: #EFECE7;
            background-color: #8C7040;
            border-color: #8C7040;
            padding: 0.5rem 1rem;
        }

        .main-title::after {
            content: '';
            width: 120px;
            height: 2px;
            position: absolute;
            bottom: -20px;
            left: 50%;
            transform: translatex(-50%);
        }
        /*start navbar*/
        .navbar {
            background-color: #1B3252;
        }

            .navbar .navbar-nav .nav-link {
                color: #EFECE7;
            }

                .navbar .navbar-nav .nav-link.active,
                .navbar .navbar-nav .nav-link:focus,
                .navbar .navbar-nav .nav-link:hover {
                    font-size: large;
                    color: var(--gary);
                }

        .navbar-toggler {
            color: #8C7040;
            font-size: 2rem;
        }

        /*end navbar*/
        /*start landing*/
        .landing {
            background-color: #1B3252;
            min-height: calc(110vh - 72px);
            background-image: url("/images/Photo1.png");
            background-repeat: no-repeat;
            background-attachment: scroll;
            background-blend-mode: normal;
            background-size: cover;
        }
        /*end landing*/

        /*start features*/


    </style>
</head>

<body>
    <script src="Js/bootstrap.bundle.min.js"></script>
    <script src="Js/all.min.js"></script>
    <nav class="navbar navbar-expand-lg sticky-top ">
        <div class="container">
            <a class="navbar-brand" href="#"><img src="/images/Logo.png"></a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span>ME<br>NU</span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav  ms-auto mb-2 mb-lg-0 ">
                    <li class="nav-item ">
                        <a class="nav-link p-2 p-lg-3 " aria-current="page"  asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3">Services</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" >About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" >Crew</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link p-2 p-lg-3" >Get In Touch</a>
                    </li>
                </ul>
               
                <a class="btn btn-success rounded-pill main-btn" asp-controller="Account" asp-action="Login">Login</a>
            </div>
        </div>
    </nav>
    <div >


        @RenderBody()



    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>